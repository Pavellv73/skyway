.block9
  width 100%
  height auto

  .block9-content
    width 100%
    height auto
    justify-content center
    align-items center
    flex-direction column
    padding-top 40px
    padding-bottom 100px
    box-sizing border-box
    @media (min-width d-screen)
      padding-bottom 130px


    .block9-head
      width 100%
      height auto
      margin-bottom 40px

    .pertners-slider
      width 100%
      height auto



      .slick-slider
        width 100%
        height 250px
        @media (min-width d-screen)
          height 340px

        .slick-next, .slick-prev
          display none!important
          &:before
            display none!important

        .slick-slide
          width 100%
          height auto
          display flex
          display -webkit-flex
          justify-content center
          align-items center
          flex-direction column
          @media (min-width d-screen)
            flex-direction row

          .video-slide
            display none
            @media (min-width d-screen)
              display block
              margin-right 32px
              width 484px
              height 272px

            iframe
              width 100%
              height 100%

          .partners-text
            width 100%
            height auto
            max-width 590px

            p
              color #232323
              font-family LatoWebLight
              font-size 14px
              font-weight 300
              text-align left
              line-height 16px
              margin-bottom 48px
              @media (min-width t-screen)
                font-size 16px
                line-height 18px
              @media (min-width d-screen)
                font-size 18px
                line-height 20px

            .name-partners
              width 100%
              height auto
              display flex
              display -webkit-flex
              justify-content space-between
              align-items center
              flex-direction row

              p
                color #232323
                font-family LatoWebBold
                font-size 14px
                font-weight 400
                margin-bottom 0

              a
                display block
                color #0095ff
                font-family LatoWebMedium
                font-size 12px
                font-weight 400
                letter-spacing 1px
                text-transform uppercase
                @media (min-width d-screen)
                  display none




        .dots-block9
          width 100%
          height auto
          display flex
          display -webkit-flex
          justify-content center
          align-items center
          flex-direction row
          margin-top 40px

          li
            width 18px
            height 18px
            border 1px solid #0095ff
            border-radius 100%
            margin-left 12px
            margin-right 12px
            transition all 0.2s ease
            cursor pointer


            button
              display none

          li.slick-active
            width 18px
            height 18px
            border 1px solid #0095ff
            background-color #0095ff
            border-radius 100%
            margin-left 12px
            margin-right 12px
            transition all 0.2s ease


#video-partners
  width 100%
  height 100%
  display none

  iframe
    width 100%
    height 100%
    @media (min-width t-screen)
      height 400px